<div class="panel">
	<ul>
		{% for node in site.pages %}
			{% if node.tags == 'basic_user_pages' %}
				{% if node.root == 'enabled' %}
					<h5><a href="{{ node.url }}">{{ node.title }}</a></h5>
				{% else %}
					<li {% if node.url == page.url %}class="active"{% endif %} ><a href="{{ node.url }}">{{ node.title }}</a></li>
				{% endif %}
			{% endif %}
		{% endfor %}
	</ul>
</div>