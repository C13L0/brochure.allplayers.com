<!--[if lt IE 7]> <html class="no-js lt-ie9 lt-ie8 lt-ie7" lang="en"> <![endif]-->
<!--[if IE 7]>    <html class="no-js lt-ie9 lt-ie8" lang="en"> <![endif]-->
<!--[if IE 8]>    <html class="no-js lt-ie9" lang="en"> <![endif]-->
<head>
  <meta http-equiv="Content-Type" content="text/html; charset=utf-8"/>
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  {% if site.safe %}
    {% assign site.url = "http://allplayers.github.com/brochure.allplayers.com" %}
  {% endif %}
  <base href="{{ site.url }}/">
  <link rel="stylesheet" href="css/app.css">
  <link href="//fonts.googleapis.com/css?family=PT+Sans+Narrow:400,700" rel="stylesheet" type="text/css">
  <!-- Le HTML5 shim, for IE6-8 support of HTML elements -->
  <!--[if lt IE 9]>
    <script src="http://html5shim.googlecode.com/svn/trunk/html5.js"></script>
  <![endif]-->
  <script src="js/jquery-1.9.1.min.js"></script>
  <script src="js/app.js"></script>
  <script src="js/foundation/modernizr.foundation.js"></script>
  <script src="js/foundation/jquery.js"></script>
  <script src="js/foundation/foundation.min.js"></script>
  <script src="js/foundation/app.js"></script>
  {% if page.osmplayer or page.video_url %}
    <script src="https://ajax.googleapis.com/ajax/libs/jqueryui/1.10.0/jquery-ui.min.js"></script>
    <!-- Include osmplayer scripts and templates -->
    {% include osmplayer %}
    {{OSMPLAYER_SCRIPTS_DEBUG}}
    {{OSMPLAYER_TEMPLATE_DEFAULT}}
  {% endif %}
  <title>AllPlayers.com</title>
</head>
